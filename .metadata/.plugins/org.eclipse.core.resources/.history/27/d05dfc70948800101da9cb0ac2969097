package com.ruby.java.ch03제어문;

public class Test22 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int a = 12;
		int b = 2;
		char op = '+';
		
		
		switch(op) {
		case '+':
			System.out.println(a+b);
			break;
		case '-':
			System.out.println(a-b);
		}
		
		/*
		 * switch 문은 switch, case, break까지 한 세트로 작성 습관화 할 것 
		 */
		
		
	}

}
